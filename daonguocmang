#include<stdio.h>
#include<conio.h>
#include<string.h>
//CHUONG TRINH DAO DOAN 
int main()
{
int i,n;
printf("Day cua ban co bao nhieu phan tu? ");
scanf("%d",&n);

int a[n];
printf("Nhap c√°c phan tu vao nao:");

for (i=0;i<n;i++)
{
printf(" Phan tu thu %d cua day la :",i+1);
scanf("%d",&a[i]);
}
int mirror[n];
//bat dau dao nguoc
for (i=0;i<n;i++)
{
mirror[i]=a[n-i-1];
printf("%d",mirror[i]);
}

getch();
return 0;
}
